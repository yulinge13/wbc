<template name="29ce2d28">
  <view class="_view data-v-b7318f0e index_page">
    <view class="_view data-v-b7318f0e index_page_top">
      <view class="_view data-v-b7318f0e header">
        <view class="_view data-v-b7318f0e header_left">
          <image src="http://www.dbl.name/wbc/static/images/logo.png" class="_image data-v-b7318f0e logo"></image>
          <view class="_view data-v-b7318f0e logo_name"> 冬宝链 </view>
        </view>
      </view>
      <view class="_view data-v-b7318f0e profit">
        <view class="_view data-v-b7318f0e profit_name">产链权益系数</view>
        <view class="_view data-v-b7318f0e profit_num">0.2%</view>
      </view>
      <view class="_view data-v-b7318f0e banner">
        <image class="_image data-v-b7318f0e banner_pic" src="http://www.dbl.name/wbc/static/images/banner.png"></image>
        <view class="_view data-v-b7318f0e banner_text">
          <image src="http://www.dbl.name/wbc/static/images/通知@2x.png" class="_image data-v-b7318f0e notice_pic"></image>
          <view class="_view data-v-b7318f0e notice"> 冬宝链v1.0正式上线了！ </view>
        </view>
        <view class="_view data-v-b7318f0e slider_lists">
          <view class="_view data-v-b7318f0e slider_list">
            <view class="_view data-v-b7318f0e slider_list_top">
              <image src="http://www.dbl.name/wbc/static/images/存储@2x.png" class="_image data-v-b7318f0e"></image>
              <view class="_view data-v-b7318f0e slider_list_name"> WBC存储量 </view>
            </view>
            <view class="_view data-v-b7318f0e slider_list_num"> {{personInfo.corpus_bill || 0}} </view>
          </view>
          <view class="_view data-v-b7318f0e slider_line"></view>
          <view class="_view data-v-b7318f0e slider_list">
            <view class="_view data-v-b7318f0e slider_list_top">
              <image src="http://www.dbl.name/wbc/static/images/收益(1)@2x.png" class="_image data-v-b7318f0e"></image>
              <view class="_view data-v-b7318f0e slider_list_name"> WBC预期收益 </view>
            </view>
            <view class="_view data-v-b7318f0e slider_list_num"> {{personInfo.corpus_point || 0}} </view>
          </view>
        </view>
      </view>
      <view class="_view data-v-b7318f0e to_buy" bindtap="handleProxy" data-eventid="{{'naG-0'}}" data-comkey="{{$k}}">
        <view class="_view data-v-b7318f0e buy_btn">
          <image src="http://www.dbl.name/wbc/static/images/购买按钮.png" class="_image data-v-b7318f0e buy_btn_pic"></image>
          <image src="http://www.dbl.name/wbc/static/images/手指.png" class="_image data-v-b7318f0e buy_pic"></image>
        </view>
      </view>
    </view>
    <view class="_view data-v-b7318f0e lists">
      <view class="_view data-v-b7318f0e list" wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'dIT-1-'+index}}" data-comkey="{{$k}}" wx:for="{{linkLists}}" wx:for-index="index" wx:for-item="i">
        <image src="{{i.path}}" class="_image data-v-b7318f0e"></image>
        <view class="_view data-v-b7318f0e list_name"> {{i.name}} </view>
      </view>
    </view>
  </view>
</template>