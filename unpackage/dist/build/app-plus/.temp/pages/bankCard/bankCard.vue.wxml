<template name="1bcddf00">
  <view class="_view data-v-cf9c685a bankCard">
    <view class="_view data-v-cf9c685a login_modal">
      <view class="_view data-v-cf9c685a fill">
        <image src="http://www.dbl.name/wbc/static/images/推荐拷贝3@2x.png" class="_image data-v-cf9c685a fill_pic"></image>
        <view class="_view data-v-cf9c685a fill_val">
          <input placeholder="请输入持卡人" value="{{fromData.name}}" bindinput="handleProxy" data-eventid="{{'ZVt-0'}}" data-comkey="{{$k}}" class="_input data-v-cf9c685a" />
        </view>
      </view>
      <view class="_view data-v-cf9c685a fill">
        <image src="http://www.dbl.name/wbc/static/images/确认@2x.png" class="_image data-v-cf9c685a fill_pic"></image>
        <view class="_view data-v-cf9c685a fill_val">
          <input placeholder="请输入银行卡号" type="number" value="{{fromData.newBankAccount}}" bindinput="handleProxy" data-eventid="{{'2Rk-1'}}" data-comkey="{{$k}}" class="_input data-v-cf9c685a" />
        </view>
      </view>
      <view class="_view data-v-cf9c685a fill">
        <image src="http://www.dbl.name/wbc/static/images/确认@2x.png" class="_image data-v-cf9c685a fill_pic"></image>
        <view class="_view data-v-cf9c685a fill_val">
          <input placeholder="请确认银行卡号" type="number" value="{{fromData.bank_account}}" bindinput="handleProxy" data-eventid="{{'spn-2'}}" data-comkey="{{$k}}" class="_input data-v-cf9c685a" />
        </view>
      </view>
      <view class="_view data-v-cf9c685a fill">
        <image src="http://www.dbl.name/wbc/static/images/密码锁@2x.png" class="_image data-v-cf9c685a fill_pic"></image>
        <view class="_view data-v-cf9c685a fill_val">
          <picker class="_picker data-v-cf9c685a input" mode="selector" bindchange="handleProxy" range="{{bankLists}}" value="{{0}}" range-key="bank_name" data-eventid="{{'zlI-3'}}" data-comkey="{{$k}}">
            <view wx:if="{{bankLists[bankListsIndex]}}" class="_view data-v-cf9c685a">{{bankLists[bankListsIndex].bank_name}}</view>
          </picker>
        </view>
      </view>
      <button class="_button data-v-cf9c685a login_btn" bindtap="handleProxy" data-eventid="{{'JkE-4'}}" data-comkey="{{$k}}">添加</button>
    </view>
    <view class="_view data-v-cf9c685a footer"> 冬宝链 </view>
  </view>
</template>