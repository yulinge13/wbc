<template name="41e0c280">
  <view class="_view data-v-8376a15a wbcMoney">
    <view class="_view data-v-8376a15a header">
      <image src="http://www.dbl.name/wbc/static/images/logo.png" class="_image data-v-8376a15a"></image>
      <view class="_view data-v-8376a15a header_cont">
        <view class="_view data-v-8376a15a header_name"> 冬宝链（WBC） </view>
        <view class="_view data-v-8376a15a header_num"> {{personInfo.balance || 0}} </view>
      </view>
    </view>
    <view class="_view data-v-8376a15a cont">
      <view class="_view data-v-8376a15a fill fill_one">
        <view class="_view data-v-8376a15a fill_title">
          <view class="_view data-v-8376a15a fill_left">
            <image src="http://www.dbl.name/wbc/static/images/银行卡1.png" class="_image data-v-8376a15a"></image>
            <view class="_view data-v-8376a15a fill_left_name"> 已绑定银行卡 </view>
          </view>
        </view>
        <view class="_view data-v-8376a15a fill_cont">
          <view class="_view data-v-8376a15a" wx:if="{{cardInfo.id}}"> {{cardInfo.bank_account}} </view>
          <view class="_view data-v-8376a15a" wx:else bindtap="handleProxy" data-eventid="{{'qqm-0'}}" data-comkey="{{$k}}"> 请添加银行卡号 </view>
        </view>
      </view>
      <view class="_view data-v-8376a15a fill fill_three">
        <view class="_view data-v-8376a15a fill_title">
          <view class="_view data-v-8376a15a fill_left">
            <image src="http://www.dbl.name/wbc/static/images/键盘1.png" class="_image data-v-8376a15a"></image>
            <view class="_view data-v-8376a15a fill_left_name"> 请输入提取WBC数量 </view>
          </view>
        </view>
        <view class="_view data-v-8376a15a fill_cont">
          <input type="number" placeholder="请输入提取数量" class="_input data-v-8376a15a input" value="{{formData.num}}" bindinput="handleProxy" data-eventid="{{'vki-1'}}" data-comkey="{{$k}}" />
        </view>
      </view>
      <view class="_view data-v-8376a15a act_num">
        <view class="_view data-v-8376a15a act">
          <view class="_view data-v-8376a15a act_name"> 实际提现： </view>
          <view class="_view data-v-8376a15a act_val"> {{actNum}} </view>
        </view>
        <view class="_view data-v-8376a15a act">
          <view class="_view data-v-8376a15a act_name"> 扣除手续费用： </view>
          <view class="_view data-v-8376a15a act_val"> {{cutNum}} </view>
        </view>
      </view>
      <view class="_view data-v-8376a15a fill fill_four">
        <view class="_view data-v-8376a15a fill_title">
          <view class="_view data-v-8376a15a fill_left">
            <image src="http://www.dbl.name/wbc/static/images/交易密码.png" class="_image data-v-8376a15a"></image>
            <view class="_view data-v-8376a15a fill_left_name"> 请输入交易密码 </view>
          </view>
        </view>
        <view class="_view data-v-8376a15a fill_cont">
          <input placeholder="请输入交易密码" password="{{true}}" class="_input data-v-8376a15a input" value="{{formData.pay_password}}" bindinput="handleProxy" data-eventid="{{'a0R-2'}}" data-comkey="{{$k}}" />
        </view>
      </view>
    </view>
    <view class="_view data-v-8376a15a btn" bindtap="handleProxy" data-eventid="{{'obU-3'}}" data-comkey="{{$k}}"> 提交 </view>
  </view>
</template>